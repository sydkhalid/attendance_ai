generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Student {
  id           Int      @id @default(autoincrement())
  name         String
  roll         String
  parentEmail  String
  image        Bytes?   // we may remove later
  embedding    Json
  createdAt    DateTime @default(now())
}

